BANCODEDADOS = "ACOES.DBF"

? "vou tentar abrir o arquivo dbf"
USE ACOES ALIAS ARQUIVO SHARED NEW
cNOME := SPACE(5)
CLS

@ 10,10 SAY "COLOCA O NOME DA ACAO :" GET cNOME
READ

ARQUIVO->(DBAPPEND())

ARQUIVO->NOME := cNOME

ARQUIVO->(DBUNLOCK())
ARQUIVO->(DBCOMMIT())

? "FECHADO ARQUIVO DE DADOS"
USE


